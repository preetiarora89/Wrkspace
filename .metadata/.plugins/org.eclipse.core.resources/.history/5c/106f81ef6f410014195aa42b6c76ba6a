import java.util.AbstractList;
import java.util.Iterator;


public class MyArrayList<T> extends AbstractList {

	@Override
	public Object get(int index) {
		
		Iterator< T> itr=this.iterator();
		for(int i=0;i<index-1;i++)
			itr.next();
		return itr.next();
			
	}

	@Override
	public int size() {
		Iterator< T> itr=this.iterator();
		int size=0;
		while(itr.hasNext())
			size++;
		return size();
	}
	

}
